{% extends 'base.html.twig' %}

{% block title %}Hello MainController!{% endblock %}

{% block body %}
    <div class="container">
        <table class="table">
            <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Title</th>
                <th scope="col">Content</th>
                <th scope="col">Action</th>
            </tr>
            </thead>
            <tbody>
            {% for data in list %}
            <tr>
                <th scope="row">{{ data.id }}</th>
                <td>{{ data.title }}</td>
                <td>{{ data.content }}</td>
                <td>@mdo</td>
            </tr>
            {% endfor %}

            </tbody>
        </table>
    </div>
{% endblock %}
